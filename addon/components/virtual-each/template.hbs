<ul class="infinite-list-content" style={{contentStyle}}>
{{#each visibleItems key='@index' as |item idx|}}
  <li class="virtual-item visible-item-{{idx}}">
    {{yield item.raw item.actualIndex item.virtualIndex}}
  </li>
{{else}}
  <li class="virtual-list-empty">
    {{yield to='inverse'}}
  </li>
{{/each}}
{{#if preloader}}
  <li class="virtual-item-preloader">
    {{app/app-preloader isVisible=true}}
  </li>
{{/if}}
</ul>
